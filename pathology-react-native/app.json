{
  "expo": {
    "name": "Thinky",
    "slug": "thinky",
    "scheme": "com.pathology",
    "version": "1.3",
    "icon": "../public/logo_pfp.png",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0",
            "extraMavenRepos": ["$rootDir/../../../node_modules/@notifee/react-native/android/libs"]
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#000000",
          "icon": "../public/logo_pfp.png"
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      "react-native-background-fetch",
      "@react-native-firebase/app"
    ],
    "splash": {
      "image": "../public/logo_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#13033d"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/e480f464-4092-46c9-8730-55d2bee70c9b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "processing",
          "remote-notification"
        ],
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.transistorsoft.fetch"
        ],
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ],
        "NSCameraUsageDescription": "Enabling camera access will allow you to take a picture to use as your avatar.",
        "NSPhotoLibraryUsageDescription": "Enabling photo library access will allow you to choose a picture to use as your avatar."
      },
      "bundleIdentifier": "com.pathology",
      "buildNumber": "10",
      "associatedDomains": ["applinks:pathology.gg"]
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "pathology.gg",
              "pathPattern": ".*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "../public/logo_pfp.png",
        "backgroundColor": "#000000"
      },
      "package": "com.pathology.gg",
      "versionCode": "6"
    },
    "web": {
      "favicon": "../public/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "0c502376-1b02-47bb-bc4a-35eb67bbe8f7"
      }
    },
    "owner": "sspenst",
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
