{
  "expo": {
    "name": "Pathology",
    "slug": "pathology-react-native",
    "version": "1.0.0",
    "icon": "../public/logo_pfp.png",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#000000"
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "@notifee/react-native"
      ],
      [
        "expo-background-fetch",
        {
          "android": {
            "minimumFetchInterval": 15
          }
        }
      ]
      
    ],
    "splash": {
      "image": "../public/logo_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "processing",
          "remote-notification"
        ],
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.transistorsoft.fetch"
        ],
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ]
      },
      "bundleIdentifier": "com.pathology"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../public/logo_pfp.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.pathology"
    },
    "web": {
      "favicon": "../public/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "2d2dcf93-a7bd-4a1b-8aab-63c9c39ef523"
      }
    }
  }
}
